package board.dto;

public class Board {
	private String id, title, contents, regdate;
	public Board() {}
	
	public Board(String id, String title, String contents, String regdate) {
		this.id = id;
		this.title = title;
		this.contents = contents;
		this.regdate = regdate;
	}
	
	@Override
	public String toString() {
		return "Board [id=" + id + ", title=" + title + ", contents=" + contents + ", regdate=" + regdate + "]";
	}
}
